{
  "name": "defenestrator/hatches-api",
  "description": "On-stream fly fishing tools API",
  "keywords": [
    "Fly fishing",
    "API"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "~5.1.0",
    "league/glide":      "~0.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.4.5",
    "phpspec/phpspec": "~2.1",
    "behat/behat": "~3.0@dev",
    "behat/mink": "~1.7@dev",
    "behat/mink-extension": "~2.0@dev",
    "laracasts/behat-laravel-extension": "dev-master#205a3d2"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "hatches\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
